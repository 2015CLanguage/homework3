#include <stdio.h>
void main()
{
	float x,y,d1,d2,d3,d4;
	printf("x=");
	scanf("%f",&x);
	printf("y=");
	scanf("%f",&y);
	d1=(x-2)*(x-2)+(y-2)*(y-2);
	d2=(x+2)*(x+2)+(y-2)*(y-2);
	d3=(x+2)*(x+2)+(y+2)*(y+2);
	d4=(x-2)*(x-2)+(y+2)*(y+2);
    if (d1<=4 || d2<=4 || d3<=4 || d4<=4)
		printf("height=10cm\n");
	else printf("height=0cm\n");
}
