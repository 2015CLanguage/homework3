#include <stdio.h>
int main()
{
    float score;
	int a;
    printf("请输入成绩（0~100）:");
    scanf("%f",&score);
    a=(int)(score/10);
    switch(a)
	{
		case 10:                
    	case 9:printf("成绩是%5.1f,等级是A\n",score);break;
    	case 8:printf("成绩是%5.1f,等级是B\n",score);break;
    	case 7:printf("成绩是%5.1f,等级是C\n",score);break;
    	case 6:printf("成绩是%5.1f,等级是D\n",score);break;
    	case 5:
    	case 4:
		case 3:
    	case 2:
    	case 1:
    	case 0:printf("成绩是%5.1f,等级是E\n",score);break;
	}
	return 0;
}
