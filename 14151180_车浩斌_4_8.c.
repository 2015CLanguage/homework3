#include <stdio.h>
int main()
{
	char grade;
    float score;
	printf("请输入您的成绩：");
	scanf("%f",&score:");
	switch(score)
	{
		case'score>=90&&score<=100':printf("A"\n");break;
		case'score>=80&&score<=89':printf("B"\n");break;
		case'score>=70&&score<=79':printf("C"\n");break;
		case'score>=60&&score<=69':printf("D"\n");break;
		case'score>=0&&score<=59':printf("E"\n");break;
		default: printf("输入错误，请输入有效成绩\n");
	}
return 0;
}
